<?xml version="1.0" encoding="utf-8"?>
<resources>

	<!-- 桌面联系人弹出窗口样式 -->
	<style name="desktop_popuwindow" parent="android:Animation"
		>
		<item name="android:windowEnterAnimation">@anim/fade_in</item>
		<item name="android:windowExitAnimation">@anim/fade_out</item>
	</style>
	<!--
		<mce:style name="PopupAnimation" parent="android:Animation"> <item
		name="android:windowEnterAnimation">@anim/popup_enter</item> <item
		name="android:windowExitAnimation">@anim/popup_exit</item>

		</mce:style> <style name="PopupAnimation" parent="android:Animation"
		mce_bogus="1"> 
		<item name="android:windowEnterAnimation">@anim/popup_enter</item> 
		<item name="android:windowExitAnimation">@anim/popup_exit</item> 
		</style>
	-->
	<style name="ContentOverlay" parent="android:style/Theme.Dialog">
 		<item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/white</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:screenOrientation">landscape</item>
    </style>
    
	<style name="Theme.greenversion2" parent="@android:style/Theme.Holo.Light">
        <item name="android:actionBarTabTextStyle">@style/greenversion2_tab</item>
    </style>

    <style name="greenversion2_tab" parent="@android:style/TextAppearance.Holo.Widget.ActionBar.Title">
        <item name="android:textColor">@color/blue</item>
        <item name="android:textSize">18sp</item>

    </style>
</resources>